<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.weber.knowledgegame">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainListActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LifeCircleActivity"
            android:theme="@style/AppTheme"
            />
        <activity android:name=".SingleTouchActivity"
            android:theme="@style/AppTheme"
            />
        <activity android:name=".MultiTouchActivity" />
        <activity android:name=".KeyActivity" />
        <activity android:name=".AccelerometerActivity" />
        <activity android:name=".AssetsActivity" />
    </application>
</manifest>
